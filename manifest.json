{
  "manifest_version": 2,

  "name": "Tab Rename",
  "description": "This extension allows the user to rename Chrome tabs.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
	"default_title": "Rename Tab"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "background": {
    "scripts":["background.js"],
    "persistent": false
  }
}